{
  "name": "@expo/contact-utils",
  "version": "0.0.0",
  "description": "Tools for working with Expo.Contacts",
  "license": "MIT",
  "readmeFilename": "README.md",
  "files": ["dist"],
  "main": "dist/index.js",
  "homepage": "https://github.com/expo/expo-contact-utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/expo/expo-contact-utils.git"
  },
  "bugs": {
    "url": "https://github.com/expo/expo-contact-utils/issues"
  },
  "author": {
    "email": "bacon@expo.io",
    "name": "Evan Bacon"
  },
  "keywords": ["react", "native", "expo", "exponent", "contacts"],
  "eslintConfig": {
    "extends": "universe/native"
  },
  "directories": {
    "example": "examples",
    "lib": "src"
  },
  "pre-push": ["lint"],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepare": "npm run build",
    "prepush": "npm run lint",
    "lint:example": "eslint example/",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "sync-example": "rsync -rv dist example/node_modules/@expo/contact-utils"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.2.0",
    "expo": "^29.0.0"
  },
  "devDependencies": {
    "babel-preset-expo": "^4.0.0",
    "eslint": "^5.1.0",
    "eslint-config-universe": "^1.0.6",
    "eslint-plugin-jest": "^21.2.0",
    "husky": "^0.14.3",
    "jest": "^23.4.1",
    "jest-expo": "^28.0.0",
    "prettier": "^1.7.4",
    "rollup": "0.57.1",
    "rollup-plugin-commonjs": "9.1.0",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-typescript2": "0.12.0"
  }
}
